+++ b/libfprint/drivers/elan.c
@@ -23,6 +23,10 @@
 #include "drivers_api.h"
 #include "elan.h"
 
+#ifndef ETIME
+#define ETIME ETIMEDOUT /* For kFreeBSD */
+#endif
